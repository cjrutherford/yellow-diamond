{
  "name": "YellowDiamond",
  "version": "1.0.0",
  "description": "Yellow diamond intends to be a drop in authentication gateway for other applications.",
  "main": "index.js",
  "author": "Chris Rutherford",
  "license": "MIT",
  "private": false,
  "scripts": {
    "client": "npm start --prefix client",
    "server":"nodemon index.js",
    "dev": "concurrently \"nodemon index.js\" \"PORT=2000 npm run client\"",
    "dev-debug": "concurrently \"nodemon --inspect index.js\" \" yarn run client\" ",
    "client:build": "cd client && yarn build",
    "start": "node index.js",
    "client-install": "yarn install --prefix client"
  },
  "dependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/dotenv": "^6.1.0",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/mongoose": "^5.3.7",
    "@types/passport": "^1.0.0",
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "bunyan": "^1.8.12",
    "bunyan-format": "^0.2.1",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.3.0",
    "keypair": "^1.0.1",
    "moment": "^2.24.0",
    "mongoose": "^5.3.0",
    "nodemailer": "^5.1.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "promise-sequential": "^1.1.1",
    "uniqid": "^5.0.3",
    "validator": "^10.8.0"
  }
}
